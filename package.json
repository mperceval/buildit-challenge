    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.3",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
